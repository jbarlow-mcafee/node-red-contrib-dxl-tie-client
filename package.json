{
  "name": "@opendxl/node-red-contrib-dxl-tie-client",
  "version": "0.1.0",
  "author": "McAfee, LLC",
  "description": "McAfee Threat Intelligence Exchange (TIE) DXL nodes for Node-RED",
  "license": "Apache-2.0",
  "keywords": [
